a=int(input("enter a num :"))
b=int(input("enter a num :"))
c=a+b
print("{0}+{1}={2}".format(a,b,c))
c=a-b
print("{0}-{1}={2}".format(a,b,c))
c=a*b
print("{0}*{1}={2}".format(a,b,c))
c=a/b
print("{0}/{1}={2}".format(a,b,c))
c=a**b
print("{0}power{1}={2}".format(a,b,c))
c=a//b
print("{0}//{1}={2}".format(a,b,c))
